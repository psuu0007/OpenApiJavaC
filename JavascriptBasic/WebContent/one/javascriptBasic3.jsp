<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>javascriptBasic3</title>
</head>

<body>
	
</body>

<script type="text/javascript">

	var a = 10;
	var b = '10';
// 	데이터의 형 상관없음
	document.write(a == b);
	document.write('<br>');
	
	document.write(a + b);
	document.write('<br>');

// 	데이터(data) 및 형(type)이 일치할 때
	document.write(a === b);
	document.write('<br>');
	
// 	데이터 및 형이 불일치할 때
	document.write(a !== b);
	document.write('<br>');
	
</script>

</html>


