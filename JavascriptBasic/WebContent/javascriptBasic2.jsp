	document.write(num + '<br/>');

	var num2 = 10;
	
	document.write(num + num2 + '<br/>');
	
	var str = "글자임";

	document.write(str + '<br/>');
	
	str = false;
	
	document.write(str + '<br/>');
	
	num = null;
	
